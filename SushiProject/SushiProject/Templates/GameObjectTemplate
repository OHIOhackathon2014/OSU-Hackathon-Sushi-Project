package 
{
	import flash.display.Sprite;
	import flash.events.Event;
	
	public class !NAME! extends Sprite 
	{
		!SPRITEFIELDS!
		
		public function !NAME!(x:Number, y:Number):void 
		{
			this.x = x;
			this.y = y;
			
			!INITSPRITE!
			
			addEventListener(Event.ADDED_TO_STAGE, AddEventModel);
		}
		
		private function AddEventModel(e:Event):void 
		{
			removeEventListener(Event.ADDED_TO_STAGE, init);
			
			!ADDSPRITE!
		}
	}
}