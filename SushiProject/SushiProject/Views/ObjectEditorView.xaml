<Window x:Class="SushiProject.Views.ObjectEditorView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ObjectEditorView" Height="600" Width="800">
    <DockPanel>
        <Menu DockPanel.Dock="Top">
            <MenuItem Header="File">
                <MenuItem Header="New"></MenuItem>
                <MenuItem Header="Open"></MenuItem>
                <MenuItem Header="Save"></MenuItem>
            </MenuItem>
            <MenuItem Header="Project">
                <MenuItem Header="Settings"></MenuItem>
            </MenuItem>
            <MenuItem Header="Compile">
                <MenuItem Header="Build and Run">
                </MenuItem>
            </MenuItem>
        </Menu>
        <DockPanel DockPanel.Dock="Right" Width="160">
            <Button DockPanel.Dock="Bottom">Save</Button>
            <Image></Image>
        </DockPanel>
        <Image></Image>
        <DockPanel>
            <ListBox>
                <ListBoxItem>
                    <DockPanel>
                        <TextBlock>update</TextBlock>
                        <ListBox>
                            <Image></Image>
                            <Image></Image>
                        </ListBox>
                    </DockPanel>
                </ListBoxItem>
            </ListBox>
        </DockPanel>
    </DockPanel>
</Window>
